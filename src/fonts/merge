#!/usr/bin/env fontforge
Open("Open_Sans/OpenSans-VariableFont_wdth,wght.ttf")
SelectAll()
ScaleToEm(1024)
Generate("/tmp/1.ttf")
Close()
Open("Inconsolata/Inconsolata-VariableFont_wdth,wght.ttf")
SelectAll()
ScaleToEm(1024)
Generate("/tmp/2.ttf")
Close()
Open("/tmp/1.ttf")
MergeFonts("/tmp/2.ttf")
Generate("fonts.ttf")
Close()
